{
    "name": "reactmore/oyindonesia-php-sdk",
    "type": "library",
    "description": "Client API Oy indonesia Busines",
    "homepage": "https://github.com/reactmore/oyindonesia-php-sdk",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Andry Setyoso",
            "homepage": "https://github.com/reactmore"
        }
    ],
    "keywords": [
        "Oyindonesia",
        "OY",
        "php",
        "payment gateway",
        "payment-library"
    ],
    "autoload": {
        "psr-4": {
            "Reactmore\\OY\\": "src/"
        }
    },
    "require": {
        "php": "^7.0 | ^8.0",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "vlucas/phpdotenv": "^4.2 || ^5.0",
        "symfony/http-foundation": "^5.3"
    },
    "require-dev": {
        "mockery/mockery": "~1.3",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "*"
    },
    "scripts": {
        "check-code": [
            "vendor/bin/parallel-lint . --exclude vendor",
            "vendor/bin/phpunit tests"
        ]
    }
}